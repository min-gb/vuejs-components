{
  "name": "@min-gb/vuejs-components",
  "version": "0.0.5",
  "private": false,
  "license": "MIT",
  "keywords": [
    "scada-lts",
    "vuejs components"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name min-gb ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "description": "## Simple vuejs components for scada-lts ## How to use: 1. Just use `npm install --save @min-gb/vuejs-components` 2. Use the components in your file directly.",
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-imask": "^6.0.5",
    "vue-moment": "^4.1.0",
    "vue-timers": "^2.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "main": "./dist/min-gb.common.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  }
}
